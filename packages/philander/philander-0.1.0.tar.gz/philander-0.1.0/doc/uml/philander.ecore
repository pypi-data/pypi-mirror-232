      <eOperations name="setReg"/>
      <eOperations name="getWReg"/>
      <eOperations name="setWReg"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ActorUnit" eSuperTypes="#//philander/EventHandler #//philander/Configurable #//pymitter/EventEmitter">
      <eOperations name="init"/>
      <eOperations name="close"/>
      <eOperations name="handleEvent"/>
      <eOperations name="setBLEDiscoveryTimeout"/>
      <eOperations name="getBLEConnectionState"/>
      <eOperations name="isCoupled"/>
      <eOperations name="couple"/>
      <eOperations name="decouple"/>
      <eOperations name="startCueing"/>
      <eOperations name="stopCueing"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Device" abstract="true">
      <eOperations name="getReg"/>
      <eOperations name="setReg"/>
      <eOperations name="modReg"/>
      <eOperations name="disableReg"/>
      <eOperations name="enableReg"/>
      <eOperations name="copyReg"/>
      <eOperations name="disableWReg"/>
      <eOperations name="enableWReg"/>
      <eOperations name="copyWReg"/>
      <eOperations name="getWReg"/>
      <eOperations name="setWReg"/>
      <eOperations name="modWReg"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Sensor" abstract="true" interface="true">
      <eOperations name="reset"/>
      <eOperations name="getID"/>
      <eOperations name="checkID"/>
      <eOperations name="testConnection"/>
      <eOperations name="nextData"/>
      <eOperations name="lastData"/>
      <eOperations name="setRange"/>
      <eOperations name="setDataRate"/>
    </eClassifiers>
