      <eOperations name="init"/>
      <eOperations name="close"/>
      <eOperations name="enable"/>
      <eOperations name="disable"/>
      <eOperations name="isRunning"/>
      <eOperations name="clear"/>
      <eOperations name="isElapsed"/>
      <eOperations name="clearElapsed"/>
    </eClassifiers>
