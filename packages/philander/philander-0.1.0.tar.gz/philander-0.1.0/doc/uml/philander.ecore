      <eOperations name="getRemainingCapacity"/>
      <eOperations name="getRemainingCapacityLevel"/>
      <eOperations name="getRemainingCapacityStr"/>
      <eOperations name="cap2level"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Configurable" abstract="true" interface="true">
      <eOperations name="Init"/>
      <eOperations name="close"/>
      <eOperations name="configure"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="InterruptSource" abstract="true" interface="true">
      <eOperations name="init"/>
      <eOperations name="close"/>
      <eOperations name="getIntStatus"/>
