      <eOperations name="read"/>
      <eOperations name="poll"/>
      <eOperations name="close"/>
      <eOperations name="setLabel"/>
      <eOperations name="asyncWait4Press"/>
      <eOperations name="stop_waiting"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SmartLED">
      <eOperations name="write"/>
      <eOperations name="on"/>
      <eOperations name="off"/>
      <eOperations name="blink"/>
      <eOperations name="stop_blinking"/>
    </eClassifiers>
  </eSubpackages>
</ecore:EPackage>
