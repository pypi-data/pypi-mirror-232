      <eOperations name="close"/>
      <eOperations name="getRemainingCapacity"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SerialDevice" abstract="true" eSuperTypes="#//philander/Device #//philander/Configurable">
      <eOperations name="readByte"/>
      <eOperations name="readWord"/>
      <eOperations name="readDWord"/>
      <eOperations name="writeByte"/>
      <eOperations name="writeWord"/>
      <eOperations name="writeDWord"/>
      <eOperations name="writeBlock"/>
