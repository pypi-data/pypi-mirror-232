import pytest
from unittest import mock

from envia.env import EnvVar

def test_can_read_environment_variable():
    os_vars = {
        "OS_VAR_1" : "some_value",
        "OS_VAR_2" : "2",
        "OS_VAR_3" : ""
    }
    
    with mock.patch.dict(os.environ, os_vars):
        v1 = EnvVar("OS_VAR_1")
