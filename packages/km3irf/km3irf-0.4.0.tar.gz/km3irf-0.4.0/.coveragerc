[run]
source = km3irf

[report]
include = 
    src/km3irf/*
omit =
    src/km3irf/version.py
    src/km3irf/__init__.py
    src/km3irf/data/
    src/km3irf/test_utils.py
exclude_lines =
    pragma: no cover
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if self.debug:
    if settings.DEBUG
    def __repr__